fn itoa(int);

struct ArrayList;

fn intcmp(int a, int b) {
    return a - b;
}

fn intrev(int a, int b) {
    return b - a;
}

pub fn main() {
    ArrayList* l = ArrayList__new(10);
    l.push(7);
    l.push(4);
    l.push(9);
    l.push(1);
    l.push(5);
    l.push(45);
    l.push(0);
    l.push(2);
    l.push(8);
    l.push(12);
    l.push(98);
    l.push(5);
    l.push(5);
    l.push(3);
    char* s = l.to_string(&itoa);
    printf("unsorted: %s\n", s);
    free(s);
    l.sort(&intcmp);
    s = l.to_string(&itoa);
    printf("  sorted: %s\n", s);
    free(s);
    l.sort(&intrev);
    s = l.to_string(&itoa);
    printf(" reverse: %s\n", s);
    free(s);
    l.drop();
}
