pub fn main() {
    int zeros = 0;
    int pos = 50;
    while !iseof() {
        char* line = read_line();
        char dir = line[0];
        int dist = atoi(line + 1);
        free(line);
        if dir == 'L' { dist = -dist; }
        pos = (pos + dist) % 100;
        if pos == 0 { zeros = zeros + 1; }
    }
    printf("Password: %d\n", zeros);
}
